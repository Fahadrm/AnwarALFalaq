<odoo>
  <data>
 <record id="view_purchase_order_qc_status" model="ir.ui.view">
        <field name="name">purchase.order.form.qc_status</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_form"/>
        <field name="arch" type="xml">

            <xpath expr="//group[@name='other_info']//field[@name='user_id']" position="after">
                <field name="qc_status"/>
                 <field name="identify" invisible="1"/>
            </xpath>
        </field>
    </record>
  </data>
</odoo>