<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_stock_received_form" model="ir.ui.view">
            <field name="name">stock.received.form</field>
            <field name="model">stock.received</field>
            <field name="arch" type="xml">
                <form string="Stock Received">
                     <sheet>
                        <group>
                            <group>
                                <field name="qty_received"/>
                            </group>
                        </group>
                     </sheet>
                </form>
            </field>
        </record>
        <record id="view_stock_received_tree" model="ir.ui.view">
            <field name="name">stock.received.tree</field>
            <field name="model">stock.received</field>
            <field name="arch" type="xml">
                <tree string="Stock Received">
<!--                    <field name="product_id"/>-->
                    <field name="qty_received"/>

                </tree>

            </field>
        </record>

        <record id="action_stock_received_form" model="ir.actions.act_window">
            <field name="name">Stock Received</field>
            <field name="res_model">stock.received</field>
            <field name="view_mode">form,tree</field>
            <field name="help" type="html">
                 <p class="o_view_nocontent_smiling_face">
                </p>
            </field>
        </record>
        <menuitem id="stock_received_menu"  name="Stock Received" sequence="5" parent="stock.product_uom_menu" action="action_stock_received_form"/>
    </data>
</odoo>