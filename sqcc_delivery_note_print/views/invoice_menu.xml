                <record id="paperformat_sqcc_delivery_note_print_with_header" model="report.paperformat">
            <field name="name">SQCC Delivery Note Print</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">45</field>
            <field name="margin_bottom">30</field>
            <field name="margin_left">4</field>
            <field name="margin_right">4</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">38</field>
            <field name="dpi">90</field>
        </record>
        <record id="sqcc_delivery_note_print_menu" model="ir.actions.report">
            <field name="paperformat_id" ref="sqcc_delivery_note_print.paperformat_sqcc_delivery_note_print_with_header"/>
        </record>


